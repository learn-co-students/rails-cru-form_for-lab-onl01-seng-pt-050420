<h3>Song Edit Form</h3>
 
<%= form_for(@song) do |f| %>
    <label>Song name:</label><br>
    <%= f.text_field :name %><br>
    <%= f.label :artist_id %>
    <%= f.number_field :artist_id %><br />
    <%= f.label :genre_id %>
    <%= f.number_field :genre_id %><br />
   
    <%= f.submit %>
  <% end %>